  "private": true,
  "dependencies": {
    "apollo-boost": "^0.1.12",
    "apollo-link-context": "^1.0.8",
    "apollo-link-ws": "^1.0.8",
    "graphql": "^0.13.2",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-apollo": "^2.1.9",
    "react-dom": "^16.4.1",
    "react-motion": "^0.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-table": "^6.8.6",
    "react-virtualized": "^9.20.1",
    "subscriptions-transport-ws": "^0.9.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"**/*.{js,jsx,graphql,json,scss,css,,md}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "husky": "^1.0.0-rc.6",
    "lint-staged": "^7.1.0",
    "prettier": "^1.12.1",
    "tachyons": "^4.11.1"
  },
  "lint-staged": {
    "*.js": [
      "yarn run prettier",
      "git add"
    ],
    "*.jsx": [
      "yarn run prettier",
      "git add"
    ],
    "*.graphql": [
      "yarn run prettier",
      "git add"
    ],
    "*.json": [
      "yarn run prettier",
      "git add"
    ],
    "*.scss": [
      "yarn run prettier",
      "git add"
    ],
    "*.css": [
      "yarn run prettier",
      "git add"
    ],
    "*.md": [
      "yarn run prettier",
      "git add"
    ]
  }
}
